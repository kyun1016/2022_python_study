   "source": [
    "I_N = int(input())\n",
    "temp_NUM = (I_N%10)*10 + (I_N//10 + I_N%10)%10\n",
    "O_cycle_length = 1\n",
    "\n",
    "while True:\n",
    "    if temp_NUM == I_N:\n",
    "        break\n",
    "    O_cycle_length += 1\n",
    "    temp_NUM = (temp_NUM%10)*10 + (temp_NUM//10 + temp_NUM%10)%10\n",
    "\n",
    "print(O_cycle_length)\n",
    "    \n",
    "\n"
   ]
  }
